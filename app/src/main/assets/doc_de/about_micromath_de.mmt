    <text_fragment textStyle="subsection">
      <term key="text" text="Das App Icon" />
    </text_fragment>
    <text_fragment>
      <term key="text" text="Das App Icon ist generiert mit Hilfe&#10;folgender Funktion, die im&#10;Polarkoordinatensystem gegeben ist:" />
    </text_fragment>
    <equation>
      <term key="leftTerm" text="f" />
      <term key="rightTerm" code="equidistant_interval">
        <term key="minValue" text="0.01" />
        <term key="nextValue" text="0.03" />
        <term key="maxValue" text="150" />
      </term>
    </equation>
    <equation>
      <term key="leftTerm" text="s(f)" />
      <term key="rightTerm" code="plus">
        <term key="leftTerm" code="plus">
          <term key="leftTerm" text="4" />
          <term key="rightTerm" code="sin">
            <term key="argTerm" code="mult">
              <term key="leftTerm" text="5" />
              <term key="rightTerm" text="f" />
            </term>
          </term>
        </term>
        <term key="rightTerm" code="plus">
          <term key="leftTerm" code="divide">
            <term key="leftTerm" code="sin">
              <term key="argTerm" code="mult">
                <term key="leftTerm" text="10" />
                <term key="rightTerm" text="f" />
              </term>
            </term>
            <term key="rightTerm" text="2" />
          </term>
          <term key="rightTerm" code="divide">
            <term key="leftTerm" code="sin">
              <term key="argTerm" code="mult">
                <term key="leftTerm" text="60" />
                <term key="rightTerm" text="f" />
              </term>
            </term>
            <term key="rightTerm" text="6" />
          </term>
        </term>
      </term>
    </equation>
    <equation>
      <term key="leftTerm" text="r(f)" />
      <term key="rightTerm" code="mult">
        <term key="leftTerm" text="0.9" />
        <term key="rightTerm" code="mult">
          <term key="leftTerm" code="plus">
            <term key="leftTerm" text="1" />
            <term key="rightTerm" code="divide_slash">
              <term key="leftTerm" text="f" />
              <term key="rightTerm" text="50" />
            </term>
          </term>
          <term key="rightTerm" code="content:com.mkulesh.micromath.link.s">
            <term key="argTerm" text="f" />
          </term>
        </term>
      </term>
    </equation>
    <equation>
      <term key="leftTerm" text="x(f)" />
      <term key="rightTerm" code="mult">
        <term key="leftTerm" code="content:com.mkulesh.micromath.link.r">
          <term key="argTerm" text="f" />
        </term>
        <term key="rightTerm" code="cos">
          <term key="argTerm" text="f" />
        </term>
      </term>
    </equation>
    <equation>
      <term key="leftTerm" text="y(f)" />
      <term key="rightTerm" code="mult">
        <term key="leftTerm" code="content:com.mkulesh.micromath.link.r">
          <term key="argTerm" text="f" />
        </term>
        <term key="rightTerm" code="sin">
          <term key="argTerm" text="f" />
        </term>
      </term>
    </equation>
    <plot_function width="680" height="650" axes_style="crossed" xLabelsNumber="7" yLabelsNumber="7" gridLineColor="#FF838383" xType="linear" yType="linear" functionsNumber="1">
      <term key="yMaxValue" text="20" />
      <term key="yFunction" color="#FF003DBB" width="3" lineStyle="solid" shapeType="none" shapeSize="300" code="content:com.mkulesh.micromath.link.y">
        <term key="argTerm" text="f" />
      </term>
      <term key="yMinValue" text="-20" />
      <term key="xMinValue" text="-20" />
      <term key="xFunction" code="content:com.mkulesh.micromath.link.x">
        <term key="argTerm" text="f" />
      </term>
      <term key="xMaxValue" text="20" />
    </plot_function>
